@model IEnumerable<Ingrediant>
@{
  ViewData["Title"] = "Ingrediants";
}

<style>
  ul {
    display: block;
    list-style-type: none;
    padding-inline-start: 0;
  }
</style>

<div class="text-center">
  <h1 class="display-4">Ingrediants</h1>
</div>

<div>
  <ul> 
    @foreach (var item in Model) {
      <h3>@item.Name</h3>
    }
  </ul>
</div>